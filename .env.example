# Development Mode
NEXT_PUBLIC_USE_MOCK_DATA=true

# Cloudflare Pages
# NEXT_PUBLIC_USE_MOCK_DATA=false

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_CLOUDFLARE_API_URL=https://test.nataliegwinters.com

# KV Configuration
# These are automatically set in Cloudflare Pages
# VIDEO_DATA=your-kv-namespace

# Feature Flags
NEXT_PUBLIC_ENABLE_SEARCH=true
NEXT_PUBLIC_ENABLE_VIDEOS=true
NEXT_PUBLIC_ENABLE_ARTICLES=true

# Cloudflare Configuration
CLOUDFLARE_API_TOKEN=your-api-token
CLOUDFLARE_ACCOUNT_ID=your-account-id

# KV Namespace IDs
VIDEO_DATA_KV_ID=your-production-kv-namespace-id
VIDEO_DATA_KV_PREVIEW_ID=your-preview-kv-namespace-id

# Environment
NODE_ENV=development 